<% include ../partials/_header %>

<div>

  <h1>User Profile</h1>
  <h2><%= user.username %></h2>

  <div>
  <% gigs.forEach((gig) => { %>
    <div>
      <%= gig.name %>
    </div>
    <div>
      <%= gig.supports %>
    </div>
    <div>
      <a href='/gigs/<%= gig.nameForUrl %>'>Link to gig page</a>
    </div>
    <div>
      Submitted by: <a href='/users/<%= gig.author.username %>'><%= gig.author.username %></a>
    </div>
  <% }) %>
</div>

<% include ../partials/_footer %>